{
  "name": "jest-ts",
  "version": "1.1.22",
  "description": "High-performance Jest TypeScript transpiler powered by esbuild, supporting type checking, intelligent caching, and one-click initialization.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jest-ts": "./dist/cli/init.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./jest-preset": {
      "require": "./dist/jest-preset.js",
      "types": "./dist/jest-preset.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache/jest-ts",
    "build": "tsup",
    "test": "jest",
    "git": "read -p 'Enter commit message: ' msg && git add . && git commit -m \"$msg\" && git push"
  },
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.6.2",
    "esbuild": "^0.20.2",
    "jest": "^30.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "jest": ">=28.0.0",
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "color-printf": "^1.1.4",
    "fs-extra": "^11.3.2",
    "path": "^0.12.7"
  },
  "keywords": [
    "jest",
    "typescript",
    "esbuild",
    "transpiler",
    "test",
    "jest-transform",
    "fast"
  ],
  "author": {
    "name": "jvy",
    "email": "jvy@qq.com",
    "url": "https://hub.js.cn"
  },
  "license": "MIT",
  "homepage": "https://hub.js.cn",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/nodets/jest-ts.git"
  }
}
